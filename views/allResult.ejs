<!DOCTYPE html>
<html>

<head>
    <title>All Results</title>
</head>

<body>
    <h2>All Student Results</h2>
    <ul>
        <% results.forEach(result=> { %>
            <li>
                <strong>
                    <%= result.studentId %>
                </strong>:
                <%= result.grade %> (
                    <%= result.percentage.toFixed(2) %>%)
            </li>
            <% }) %>
    </ul>
</body>

</html>